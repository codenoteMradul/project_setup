
<h1>Fill The Form</h1>

<%= form_with(model: @users) do |form| %>

<div>
	<%= form.label :name %><br>
	<%= form.text_area :name %>
</div>

<div>
	<%= form.label :email %><br>
	<%= form.text_area :email %>
</div>

<div>
	<%= form.label :password %><br>
	<%= form.text_area :password %>
</div>

<div>
	<%= form.label :age %><br>
	<%= form.text_area :age %>
</div>

<div>
	<%= form.label :address %><br>
	<%= form.text_area :address %>
</div>

<div>
	<%= form.submit %>
</div>

<% end %>
<br>



